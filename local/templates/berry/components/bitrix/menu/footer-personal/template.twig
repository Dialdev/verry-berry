{% spaceless %}
    {% if result %}
        <div class="footer__navWrapper footer__navWrapper_lk">
            <div class="footer__title">Личный кабинет</div>
            <div class="footer__nav footer__nav_gray">
                {% for item in result %}
                    <a class="footer__navItem" href="{{ item.LINK }}">{{ item.TEXT }}</a>
                {% endfor %}
            </div>
        </div>
    {% endif %}
{% endspaceless %}
